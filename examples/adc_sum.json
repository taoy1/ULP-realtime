{
	"//": "Sensor 1 reads data from ADC channel 6 at 33.3Hz",
	"sensor 1": {
		"interval_in_us": 30000,
		"input": "ADC_6",
		"op": "null"
	},
	"//": "Sensor 2 does nothing at 50Hz",
	"sensor 2": {
		"interval_in_us": 50000,
		"input": "null",
		"op": "null"
	},
	"//": "Sensor 3 does nothing at 16.6Hz",
	"sensor 3": {
		"interval_in_us": 60000,
		"input": "null",
		"op": "null"
	},
	"//": "Sensor 4 sums up 10 data from Sensor 1 at 3.33Hz",
	"sensor 4": {
		"interval_in_us": 300000,
		"input": "sensor 1",
		"op": "sum"
	},
	"//": "CPU prints data in Sensor 4's buffer collected during this period of CPU sleeping",
	"CPU": {
		"interval_in_us": 1500000,
		"input": "sensor 4",
		"op": "print"
	}
}